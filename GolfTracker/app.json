{
  "expo": {
    "name": "Daddy Caddy",
    "slug": "daddy-caddy",
    "scheme": "daddycaddy",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/daddy_caddy_logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/daddy_caddy_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.daddycaddy.golf",
      "buildNumber": "2",
      "infoPlist": {
        "NSContactsUsageDescription": "Daddy Caddy needs access to your contacts to select recipients for golf score updates and messages."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/daddy_caddy_logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.daddycaddy.golf",
      "versionCode": 1,
      "permissions": [
        "READ_CONTACTS"
      ]
    },
    "web": {
      "favicon": "./assets/daddy_caddy_logo.png"
    },
    "plugins": [
      "expo-dev-client"
    ],
    "extra": {
      "eas": {
        "projectId": "51d0c2b9-e841-417f-b6b7-682603ce5779"
      }
    },
    "owner": "charbo"
  }
}
